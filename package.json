{
	"name": "pseudoanon",
	"packageManager": "yarn@3.2.0",
	"scripts": {
		"build": "tsc",
		"lint": "eslint src/**/*.ts",
		"format": "eslint src/**/*.ts --fix",
		"start": "yarn run build && node --trace_gc --max-old-space-size=8192 --inspect dist/index.js",
		"docker-build": "docker build -t docker.taktik.be/rus-backend:$npm_package_version .",
		"deploy": "yarn docker-build && docker push docker.taktik.be/rus-backend:$npm_package_version"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn format"
		}
	},
